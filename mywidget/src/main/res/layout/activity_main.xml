<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    tools:context="com.dhq.mywidget.MainActivity"
    android:orientation="vertical">

    <com.dhq.mywidget.selectview.SelectView
        android:id="@+id/main_selectview"
        android:layout_width="match_parent"
        android:layout_height="80dp" />

    <com.dhq.mywidget.selectview.WheelView
        android:id="@+id/main_wheelview"
        android:layout_width="80dp"
        android:layout_height="300dp" />


    <com.dhq.mywidget.circleprograss.CircleProgressView
        android:id="@+id/main_circleprogressView"
        android:layout_width="300dp"
        android:layout_height="300dp" />

</LinearLayout>
